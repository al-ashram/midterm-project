<div class="container">
<ul class="nav nav-tabs">
  <li class="active"><a href="#tab_a" data-toggle="tab">Pictures Table</a></li>
  <li><a href="#tab_b" data-toggle="tab">Pairs Table</a></li>
</ul>
<div class="tab-content">
  <div class="tab-pane active" id="tab_a">
    <table id="table" class="display" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Pic ID</th>
          <th>Picture</th>
        </tr>
      </thead>
      <tbody>
        <% @all_pictures.each do |picture| %>
          <tr>
            <td><%= picture.id %></td>
            <td><img src=<%= picture.url_link %> alt="face picture"></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="tab-pane" id="tab_b">
    <table id="table" class="display" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Pair ID</th>
          <th>Pic 1 ID</th>
          <th>Picture 1</th>
          <th>Pic 2 ID</th>
          <th>Picture 2</th>
          <th>Percent Match</th>
        </tr>
      </thead>
      <tbody>
        <% @all_pairs.each do |pair| %>
          <tr>
            <td><%= pair.id %></td>
            <td><%= pair.picture_one.id %></td>
            <td><img src=<%= pair.picture_one.url_link %> alt="face picture"></td>
            <td><%= pair.picture_two.id %></td>
            <td><img src=<%= pair.picture_two.url_link %> alt="face picture"></td>
            <td><%= pair.score %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>   
</div>
</div>






